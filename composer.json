{
    "name": "arcanedev/geo-ip",
    "description": "GEO IP Helper",
    "keywords": ["geo", "ip", "localization", "nation", "country"],
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "phpunit/phpcov": "~2.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
